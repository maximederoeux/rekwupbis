<%= simple_form_for(@return_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :return_box_id %>
    <%= f.input :box_id %>
    <%= f.input :dirty %>
    <%= f.input :sealed %>
    <%= f.input :clean %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
