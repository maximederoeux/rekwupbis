<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Event name</th>
      <th>Event start date</th>
      <th>Event end date</th>
      <th>Expected pax</th>
      <th>Last pax</th>
      <th>Post code</th>
      <th>City</th>
      <th>Country</th>
      <th>Comment</th>
      <th>Cuptwenty</th>
      <th>Cuptwentyfive</th>
      <th>Cupforty</th>
      <th>Cupfifty</th>
      <th>Cuplitre</th>
      <th>Cupwine</th>
      <th>Cupcava</th>
      <th>Cupshot</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.event_start_date %></td>
        <td><%= event.event_end_date %></td>
        <td><%= event.expected_pax %></td>
        <td><%= event.last_pax %></td>
        <td><%= event.post_code %></td>
        <td><%= event.city %></td>
        <td><%= event.country %></td>
        <td><%= event.comment %></td>
        <td><%= event.cuptwenty %></td>
        <td><%= event.cuptwentyfive %></td>
        <td><%= event.cupforty %></td>
        <td><%= event.cupfifty %></td>
        <td><%= event.cuplitre %></td>
        <td><%= event.cupwine %></td>
        <td><%= event.cupcava %></td>
        <td><%= event.cupshot %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
