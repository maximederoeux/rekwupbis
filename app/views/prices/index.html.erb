<p id="notice"><%= notice %></p>

<h1>Listing Prices</h1>

<table>
  <thead>
    <tr>
      <th>Article</th>
      <th>User</th>
      <th>Wash</th>
      <th>Handwash</th>
      <th>Handling</th>
      <th>Deposit</th>
      <th>Regular</th>
      <th>Negociated</th>
      <th>Valid from</th>
      <th>Valid till</th>
      <th>Sell</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prices.each do |price| %>
      <tr>
        <td><%= price.article_id %></td>
        <td><%= price.user_id %></td>
        <td><%= price.wash %></td>
        <td><%= price.handwash %></td>
        <td><%= price.handling %></td>
        <td><%= price.deposit %></td>
        <td><%= price.regular %></td>
        <td><%= price.negociated %></td>
        <td><%= price.valid_from %></td>
        <td><%= price.valid_till %></td>
        <td><%= price.sell %></td>
        <td><%= link_to 'Show', price %></td>
        <td><%= link_to 'Edit', edit_price_path(price) %></td>
        <td><%= link_to 'Destroy', price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Price', new_price_path %>
