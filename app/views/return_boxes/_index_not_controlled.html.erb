                    <% end %>
                  <br>
                  <%= return_box.return_date.strftime("%d/%m/%Y") %><br>
                  <%= return_box.delivery.total_boxes %> <%= t("menu.boxes") %> 
                </strong>
              </li>
          <% end %>
        </ul>
      </div>

      <div class="col-md-6">
        <ul class="list-group">
          <li class="list-group-item text-center">
            <h4><%= t("delivery.gone_three_days") %></h4>
          </li>

            <% @not_controlled.three_days_ago.each do |return_box| %>


