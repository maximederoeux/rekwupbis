<ul class="list-group">
	<li class="list-group-item list-group-item-warning">
		<div class="row">
			<strong>
				<div class="col-xs-8 col-md-4">
					<%= t("return.lln") %>
				</div>

				<div class="col-xs-2 col-md-4 text-center">
					<%= Date.today.strftime("%d/%m/%Y") %>
				</div>

				<div class="col-xs-2 col-md-4 text-center">
					<%= LlnImport.sum('return_box') %>
				</div>
			</strong>
		</div>
	</li>


<% @lln_imports.each do |import| %>

	<% if import.return_box >= 1 %>

		<li class="list-group-item">
			<div class="row text-center">
				<strong>
					<div class="col-xs-2 col-md-4">
						<%= import.id %>
					</div>

					<div class="col-xs-8 col-md-4">
						<%= import.circle %>
					</div>

					<div class="col-xs-2 col-md-4">
						<%= import.return_box %>
					</div>
				</strong>
			</div>
		</li>

	<% end %>
<% end %>
</ul>
<hr>

<ul class="list-group">
	<li class="list-group-item list-group-item-warning">
		<div class="row text-center">
			<strong>
				<div class="col-xs-8 col-md-8">
					<%= t("return.others") %>
				</div>

				<div class="col-xs-4 col-md-4">
					<%= Date.today.strftime("%d/%m/%Y") %>
				</div>


			</strong>
		</div>
	</li>

<% ReturnBox.this_day.each do |return_box| %>

	<% if return_box.delivery.is_not_lln %>

		<li class="list-group-item">
			<div class="row text-center">
				<strong>
					<div class="col-xs-8 col-md-8">
						<%= return_box.offer.organizer.name %>
					</div>

					<div class="col-xs-4 col-md-4">
						<%= return_box.total_return %>
					</div>

				</strong>
			</div>
		</li>

	<% end %>
<% end %>

</ul>