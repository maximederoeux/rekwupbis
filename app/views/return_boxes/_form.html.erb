<%= simple_form_for(@return_box) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :delivery_id %>
    <%= f.input :return_time %>
    <%= f.input :is_back %>
    <%= f.input :receptionist %>
    <%= f.input :ctrl_time %>
    <%= f.input :ctrler %>
    <%= f.input :is_controlled %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
