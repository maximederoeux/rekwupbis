  <div class="row">
    <ul class="list-group">
            <li class="list-group-item list-group-item-warning ">
              <h4><%= content_tag(:i, "", class: "fa fa-tint fa-2x") %>  <%= t("wash.started") %></h4>
            </li>
    </ul>
  </div>

  <div class="row">
    <div class="col-md-6">    
      <div class="col-md-6">
        <ul class="list-group">
          <li class="list-group-item text-center">
            <h4><%= t("delivery.gone_today") %></h4>
          </li>
        
            <% @started.started_this_day.each do |wash| %>


                <li class="list-group-item text-center">
          
                <strong>
                  <%= wash.offer.event.event_name %><br>
                    <% if wash.offer.organizer.is_lln %>
                      <%= wash.offer.organizer.name %><br>
                    <% end %>

                    <%= link_to wash_path(wash) do %>
                      <%= content_tag(:i, "", class: "fa fa-tint fa-3x") %>
                    <% end %>
                  
                  <br>
                  <%= t("wash.back") %><%= wash.offer.return_boxes.first.return_time.strftime("%d/%m/%Y") %><%= t("word.at") %><%= wash.offer.return_boxes.first.return_time.strftime("%H:%M") %><br>
                  <%= wash.offer.delivery.total_boxes %> <%= t("menu.boxes") %><br>
                  <%= t("wash.started2") %><%= wash.start_time.strftime("%H:%M") %>

                  <%= simple_form_for(wash) do |f| %>
                    <%= f.error_notification %>

                      <%= f.hidden_field :end_time, :value => Time.now %>
                      <%= f.hidden_field :ender, :value => current_user.id %>

                      <%= f.submit t("wash.stop"), class: "btn btn-info" %>

                  <% end %>

                </strong>
                </li>
            <% end %>
        </ul>
      </div>

