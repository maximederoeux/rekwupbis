<%= simple_form_for(@box) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :box_name %>
    <%= f.input :box_regular %>
    <%= f.input :box_type %>
    <%= f.input :box_detail_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
