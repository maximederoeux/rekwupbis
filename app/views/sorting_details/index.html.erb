<ul class="list-group">
	<li class="list-group-item">
		
	</li>
		<% @cups.each do |article| %>

			<li class="list-group-item">
				<div class="row">
					<div class="col-md-2">
						<%= article.full_name %>
					</div>
					<div class="col-md-2">
						<% unless article.total_february == 0 %>
							<%= article.total_february %>
						<% end %>				
					</div>
					<div class="col-md-2">
						<% unless article.total_march == 0 %>
							<%= article.total_march %>
						<% end %>		
					</div>
					<div class="col-md-2">
						
					</div>
					<div class="col-md-2">
						
					</div>
					<div class="col-md-2">
						
					</div>					
				</div>
			</li>

		<% end %>
</ul>



	
	<br>




<% @march.each do |sorting| %>

	<%= sorting.id %>
	<%= sorting.created_at.strftime('%d/%m/%Y') %><br>

<% end %>