                  <%= sorting.return_box.delivery.total_boxes %> <%= t("menu.boxes") %><br>

	                  <% if sorting.return_box.washes.where.not(:start_time => nil).count == 0 %>
	                  	<%= t("wash.no_wash") %>
	                  <% elsif sorting.return_box.washes.where.not(:start_time => nil).count == 1 %>
	                  	<%= sorting.return_box.washes.where.not(:start_time => nil).count %>
	                  	<%= t("wash.wash") %>
	                  <% elsif sorting.return_box.washes.where.not(:start_time => nil).count >= 2 %>
											<%= sorting.return_box.washes.where.not(:start_time => nil).count %>
											<%= t("menu.washes") %>
	                  <% end %>

                </strong>
                </li>
            <% end %>       
        </ul>
      </div>
      
    </div>  

        
