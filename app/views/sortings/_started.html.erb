  <div class="row">
    <ul class="list-group">
            <li class="list-group-item list-group-item-warning ">
              <h4><%= t("sorting.started") %></h4>
            </li>
    </ul>
  </div>

  <div class="row">
    <div class="col-md-6">    
      <div class="col-md-6">
        <ul class="list-group">
          <li class="list-group-item text-center">
            <h4><%= t("delivery.gone_today") %></h4>
          </li>
        
            <% @started.started_this_day.each do |sorting| %>


                <li class="list-group-item text-center">
          
                <strong>
                  <%= sorting.return_box.delivery.offer.event.event_name %><br>
                    <% if sorting.return_box.delivery.offer.organizer.is_lln %>
                      <%= sorting.return_box.delivery.offer.organizer.name %><br>
                    <% end %>

                    <%= link_to sorting_path(sorting) do %>
                      <%= content_tag(:i, "", class: "fa fa-eye") %>
                    <% end %>
                  
                  <br>
                  <%= t("sorting.back") %><%= sorting.return_box.return_time.strftime("%d/%m/%Y") %><%= t("word.at") %><%= sorting.return_box.return_time.strftime("%H:%M") %><br>
                  <%= sorting.return_box.delivery.total_boxes %> <%= t("menu.boxes") %><br>
                  <%= t("sorting.started2") %><%= sorting.start_time.strftime("%H:%M") %> 
                </strong>
                </li>
            <% end %>
        </ul>
      </div>

      <div class="col-md-6">
